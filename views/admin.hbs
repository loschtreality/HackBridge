<!-- Nav tabs -->
<ul class="nav nav-tabs" role="tablist">
  <li class="nav-item">
    <a class="nav-link" data-toggle="tab" href="#applicants" role="tab">Applicants</a>
  </li>
  <li class="nav-item">
    <a class="nav-link" data-toggle="tab" href="#volunteers" role="tab">Volunteers</a>
  </li>
</ul>

<!-- Tab panes -->


<div class="tab-content">
  <div class="tab-pane active" id="applicants" role="tabpanel">
    <table class="table table-striped">
      <thead>
        <tr>
          <th>Applicant Name</th>
          <th>Status</th>
          <th>Document</th>
        </tr>
      </thead>
      <tbody>
        {{#each this}}
       
            <tr>
              <td>{{applicant_name}}</td>
              <td>{{applicant_signed}}</td>
              <td>{{applicant_docusign}}</td>
            </tr>
        
        {{/each}}
      </tbody>
    </table>
  </div>
  <div class="tab-pane" id="volunteers" role="tabpanel">
    <table class="table table-striped">
      <thead>
        <tr>
          <th>Volunteer Name</th>
          <th>Status</th>
          <th>Document</th>
        </tr>
      </thead>
      <tbody>
        {{#each this}}
         
            <tr>
              <td>{{volunteer_name}}</td>
              <td>{{volunteer_status}}</td>
              <td>{{volunteer_doc}}</td>
            </tr>
      
        {{/each}}
      </tbody>
    </table>
  </div>
  <button class="btn btn-primary" id="logout" role="tabpanel">Logout</button>
</div>